<div class="home-container">
  <div>
    <div class="home-title-container">
      <%= image_tag 'community.jpg', class:"home-img" %>
      <h1>Parentaid</h1>
      <p>Share with friends and family your journey as a parent</p>
    </div>
    <div class="home-btn-container">
      <div class="d-flex mb-3">
        <%= link_to "Login", new_user_session_path, class:"main-btn" %>
      </div>
      <div class="d-flex">
        <%= link_to "Sign Up", new_user_registration_path, class:"main-btn" %>
      </div>
    </div>
    <div class="home-subtitle-container">
      <ul>
        <li>Create your own community</li>
        <li>Share your parenting experience with your friends</li>
        <li>Log your baby journal</li>
      </ul>
    </div>
  </div>
</div>
